<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatingActionButton">
        <attr name="color" format="color"/>
        <attr name="color_pressed" format="color"/>
        <attr name="icon" format="reference"/>
        <attr name="fab_height" format="integer"/>
        <attr name="fab_width" value="integer"/>
    </declare-styleable>
    <declare-styleable name="FloatingActionsMenu">
        <attr name="color_menu" format="color"/>
        <attr name="color_pressed_menu" format="color"/>
        <attr name="animation_distance" format="float"/>
        <attr name="fab_position" format="enum">
            <enum name="bottom" value="0" />
            <enum name="top" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="Toolbar">
        <attr name="color_toolbar" format="color"/>
        <attr name="firstIcon" format="integer" />
        <attr name="secondIcon" format="integer" />
        <attr name="thirdIcon" format="integer" />
    </declare-styleable>
    <declare-styleable name="ImageLayoutItemMenuItem">
        <attr name="text" format="string"/>
    </declare-styleable>
</resources>
